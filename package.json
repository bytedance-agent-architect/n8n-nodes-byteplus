{
  "name": "n8n-nodes-byteplus",
  "version": "0.1.2",
  "description": "n8n community node for BytePlus AI services (Image Generation, Video Generation, TikTok Publishing)",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "byteplus",
    "ai",
    "image-generation",
    "video-generation",
    "seedream",
    "seedance",
    "tiktok"
  ],
  "license": "MIT",
  "homepage": "https://byteplus.com/",
  "author": "Wilson Wong <wilson.wong@bytedance.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bytedance-agent-architect/n8n-nodes-byteplus.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/nodes/BytePlus/BytePlus.node.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\"",
    "lintfix": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" --fix",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/BytePlusApi.credentials.js",
      "dist/credentials/TikTokOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/BytePlus/BytePlus.node.js",
      "dist/nodes/TikTok/TikTok.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.6",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
